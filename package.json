    "collectCoverageFrom": [
      "**/utils/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "codeclimate-test-reporter": "^0.5.1",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "knex-cleaner": "^1.3.0",
    "pg": "^8.2.1"
  },
  "resolutions": {
    "open": "^6.0.0",
    "lodash": "^4.17.11"
